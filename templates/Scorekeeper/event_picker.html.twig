{% extends 'Scorekeeper/base.html.twig' %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <div class="list-group">
                {% for event in events %}
                    <a href="{{ path('scorekeeper.editor.event', {'code': code, 'eventId': event.id}) }}" class="list-group-item list-group-item-action">{{ event }}</a>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}