{% endblock %}

{% block body %}
    <div class="container-fluid" style="font-size: 2.5rem;">
        <div class="row">
            <div class="col-md-12 text-center">
                <img src="/images/stemgames-logo.png" class="logo">
            </div>
        </div>
        {% block content %}
        {% endblock %}
    </div>

{% endblock %}